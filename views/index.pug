extends frontlayout

block content
  div.main
   div.toprow
     div(class='info')
       div(class='name',class='companyname') Team Innovation LLC
       div(class='titlecontactbox')
         div(class='title') Team Optimization, Training, Coaching
         div(class='subtitle') TDD, BDD, Scrum, Lean, SAFe SPC 6
         div(class='zontline') <hr/>
         div(class='contactbox') 
           div(class='contact')
             img(class='phoneicon', src='https://tomboulet.com/image/phone-42-xxl.png', alt='Mobile Phone Icon')
             | +1 502-974-1888
           div(class='contact')
             a(href='mailto:tom@teaminnovationllc.com',target='_blank')
               img(class='emailicon',src='https://tomboulet.com/image/email-icon-99.png',alt='Email Icon')
