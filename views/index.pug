           div(class='contact')
             a(href='http://www.linkedin.com/tomboulet',target='_blank')
               img(class='linkedinicon',src='https://tomboulet.com/image/LI-In-Bug.png',alt='LinkedIn Icon')
               | &nbsp; LinkedIn
           div(class='contact')
             a(href='https://tomboulet.com/resume/Boulet_Tom_Resume_2024.pdf',target='_blank')
               img(class='pdficon',src='https://tomboulet.com/image/PDF_32.png',alt='PDF Icon')
               | &nbsp; Resume 
     div(class='imagebox')
       img(alt='Tom Boulet, Agile Coach',src='https://tomboulet.com/image/Tom_Boulet2.png',loading='lazy',width='300') 
   div(class='middlerow')   
