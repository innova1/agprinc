extends layout

block content
  div#outer
    div#menu
      each f in frameworks
        a.menu(href='/agileframeworks/' + f.framework) #{f.frameworkdisplay}
    h1 #{title} for #{f} the #{framework} framework
    table
      each pv in principlesArray
        tr(class=pv.type)
          td.text(class=pv.type) #{pv.id}: #{pv.text}
    p first item of search array is 
    p #{searchMap.get('customer').keyword}
    p found in framework: #{searchMap.get('continuous').locations[0].framework} of type: #{searchMap.get('continuous').locations[0].type} with id: #{searchMap.get('continuous').locations[0].id}
    p found in framework: #{searchMap.get('continuous').locations[1].framework} of type: #{searchMap.get('continuous').locations[1].type} with id: #{searchMap.get('continuous').locations[1].id}